{# empty Twig template #}
    <table>
        <thead><tr><th>Credit Account</th><th>Debit Account</th><th>Amount</th></tr></thead>
        <tbody>
    {%  for transaction in transactions %}
        {%  if transaction.amount > 0 %}
        <tr><td class="left">{{ transaction.creditAccount}}</td><td class="left">{{ transaction.debitAccount}}</td><td class="numbers" >{{ transaction.amount|format_currency('USD',{fraction_digit:0}) }}</tr>
        {% endif %}
    {%  endfor %}
        </tbody>
    </table>

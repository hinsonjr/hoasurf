{{ form_start(form) }}
    <div class="row">
       <div class="col">
        {{ form_row(form.name) }}
       </div>        
       <div class="col">
        {{ form_row(form.startDate) }}
       </div>        
       <div class="col">
        {{ form_row(form.endDate) }}
       </div>        
    </div>
        {{ form_row(form.address) }}
        {{ form_row(form.address2) }}
        {{ form_row(form.city) }}
        {{ form_row(form.state) }}
        {{ form_row(form.zip) }}
        {{ form_row(form.phone) }}
{#        {{ form_widget(form) }}#}
    <button class="btn btn-primary">{{ button_label|default('Save') }}</button>
{{ form_end(form) }}

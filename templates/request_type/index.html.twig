{% extends 'admin-base.html.twig' %}

{% block title %}Request Type List{% endblock %}

{% block content %}
    {{ parent() }}
    <h1>Request Type List</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Type</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for request_type in request_types %}
            <tr>
                <td>{{ request_type.id }}</td>
                <td>{{ request_type.type }}</td>
                <td>
                    <a href="{{ path('app_request_type_show', {'id': request_type.id}) }}">show</a>
                    <a href="{{ path('app_request_type_edit', {'id': request_type.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="3">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_request_type_new') }}">Create new</a>
{% endblock %}
